  set rlimit fsize <= 64M,
  set rlimit nproc <= 500,
  set rlimit as <= 1536M,

# 1 greater than max
  set rlimit nofile <= 501,

  /usr/lib/jvm/java-7-openjdk-amd64/bin/javac rix,
  /usr/lib/jvm/java-8-openjdk-amd64/bin/javac rix,
  /etc/java-*-openjdk/** r,
  /usr/lib/jvm/*/jre/bin/* rix,
  /usr/share/java*/**/*.jar r,
  /usr/share/java*/*.jar r,
